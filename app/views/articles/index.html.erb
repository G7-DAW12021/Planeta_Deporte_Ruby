<h1>Articles</h1>
<table>
  <tr>
    <th>Foto</th>
    <th>Fecha</th>
    <th>Titulo</th>
    <th>Subtitulo</th>
    <th>Seccion</th>
    <th>Cuerpo</th>
  </tr>

  <tbody>
  <% @articles.each do |i| %>
    <tr>
      <td><%= i.foto %></td>
      <td><%= i.fecha %></td>
      <td><%= i.titulo %></td>
      <td><%= i.subtitulo %></td>
      <td><%= i.seccion %></td>
      <td><%= i.cuerpo %></td>
      <td><%= link_to 'Show', i %></td>
      <td><%= link_to 'Edit', edit_article_path(i) %></td>
      <td><%= link_to 'Destroy', i, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Article', 'articles/new' %>